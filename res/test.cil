fn fact(n: i32) -> i32 {
    if(n == 0) return 1;
    return n * fact(n - 1);
}

fn feb(n: i32) -> i32 {
    if(n < 2) {
        return 1;
    } else {
        return feb(n - 1) + feb(n - 2);
    }
}

fn while_test(n: i32) -> i32 {
    var i: i32 = 0;
    var res: i32 = 0;
    while(i < 10) {
        res = res + i;
        i = i + 1;
    }
    return res;
}

fn add(a: i32, b: i32) -> i32 {
    return a + b;
}

fn main () {
    val a: i32 = getint();
    val b: i32 = getint();
    print(add(a, b));
    
    val x: i32 = getint();
    print(while_test(x));
    
    val n: i32 = getint();
    val res: i32 = fact(n);
    print(res);
    
    val m: i32 = getint();
    print(feb(m));
}